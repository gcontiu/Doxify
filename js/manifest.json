{

  "manifest_version": 2,
  "name": "measureTimeSpent",
  "version": "1.0",

  "description": "Measure time spent on specific we pages",

  "permissions": [
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["*://wiki.dev.worldpay.com/*"],
      "js": ["measureTimeSpentOnPage.js"]
    }
  ]

}