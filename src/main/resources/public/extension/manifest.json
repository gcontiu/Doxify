{

  "manifest_version": 2,
  "name": "measureTimeSpent",
  "version": "1.0",

  "description": "Measure time spent on specific we pages",

  "permissions": [
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["*://weblog.endava.com/*"],
      "js": ["timeme.js","handler.js"]
    }
  ]

}